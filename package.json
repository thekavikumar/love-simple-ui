{
  "name": "love-simple-ui",
  "version": "1.0.19",
  "description": "An open-source UI library for developers to make their life easier :)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/thekavikumar/love-simple-ui.git"
  },
  "homepage": "https://love-simple-ui.vercel.app/",
  "size-limit": [
    {
      "limit": "1 s",
      "path": "dist/index*.js",
      "import": "{ createComponent }"
    }
  ],
  "scripts": {
    "test": "jest && eslint .",
    "ladle": "ladle serve",
    "build": "tsup && size-limit",
    "start": "tsup --watch"
  },
  "keywords": [
    "react",
    "react-components",
    "react-ui",
    "ui-components",
    "components"
  ],
  "author": "Kavi Kumar",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "devDependencies": {
    "@ladle/react": "^2.12.3",
    "@size-limit/preset-big-lib": "^8.2.6",
    "@types/react": "^18.2.1",
    "@types/react-dom": "^18.2.2",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.24",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^8.2.6",
    "tailwindcss": "^3.3.2",
    "tsup": "^7.1.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "goober": "^2.1.13",
    "tailwind-merge": "^1.13.2"
  },
  "peerDependencies": {
    "react": "^16.8 || ^17 || ^18",
    "react-dom": "^16.8 || ^17 || ^18"
  },
  "bugs": {
    "url": "https://github.com/thekavikumar/love-simple-ui/issues"
  }
}
